{
  "name": "nen-core",
  "version": "1.0.0",
  "description": "Nyteshade Enterprises NodeJS - Core Library",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist; babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyteshade/nen-core.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "library"
  ],
  "author": "Brielle Harrison",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nyteshade/nen-core/issues"
  },
  "homepage": "https://github.com/nyteshade/nen-core#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "flow-bin": "^0.83.0",
    "jest": "^23.5.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-function-bind",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "deep-equal": "^1.0.1",
    "weighted-randoms": "^1.0.0"
  }
}
